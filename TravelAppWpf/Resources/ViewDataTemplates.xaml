<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TravelAppWpf.Resources"
                    xmlns:vm="clr-namespace:TravelAppWpf.ViewModels"
                    xmlns:v="clr-namespace:TravelAppWpf.Views">

    <DataTemplate DataType="{x:Type vm:SignInViewModel}">
        <v:SignInView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TripsViewModel}">
        <v:TripsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CitiesViewModel}">
        <v:CitiesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TicketsViewModel}">
        <v:TicketsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CheckListViewModel}">
        <v:CheckListView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RegisterViewModel}">
        <v:RegisterView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AddTripViewModel}">
        <v:AddTripView/>
    </DataTemplate>
    
</ResourceDictionary>